# <<<--- Main Function ! --->>>

__main__() {
interrupt
while true
do
if [[ ${TRUE} == "false" ]]
then
  FALSE=true
  if [[ ! ${CMD} == "use :"* ]]
  then
    unset FALSE
    getCOMMANDS
    prism
    TRUE=true
    break & readCONFIGURE
  fi
  getCOMMANDS
fi

if [[ ${FALSE} == true ]]
then
unset prism
prism(){ :
}

MOD="${CMD#use :}"
MOD=${MOD%/*}
INPUT1=${MOD%%/*}
INPUT2=${MOD##*/}

findMODULES $INPUT1 $INPUT2

if [[ ${MODULE_true} = ${SUBMODULE_true} ]]
then
  :
else
theERROR
unset FALSE
  prism()
  {
    __prism__
  }
fi

fi

if [[ ! ${FALSE} == "false" ]]
then
  prism
  getCOMMANDS
else
  break & readCONFIGURE
  cmdFALSE
fi
if [[ ${TRUE} == "true" ]]
then
  break & readCONFIGURE
fi
input=""
theERROR
done
}

sudoPERM
__main__

# <<<--- End --->>>
